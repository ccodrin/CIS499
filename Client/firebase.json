    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
          "source": "**",
          "destination": "/index.html"
      }
  ],
  "redirects": [ {
    "source": "https://lander-parksmart.web.app/",
    "destination": "https://lander-parksmart.web.app/home",
    "type": 301
  } ]
  }
  
}
